
generator client {
  provider = "prisma-client-js"
}

generator dbml {
    provider = "prisma-dbml-generator"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id        String       @id @default(cuid())
  email     String       @unique
  password  String
  phone     String?
  address   String?
  name      String
  createAt  DateTime      @default(now())
  updateAt  DateTime?     @updatedAt

  @@map("users")
}

